{
  "name": "induction-motor-testing-app",
  "productName": "Induction Motor Testing",
  "version": "1.0.0",
  "description": "A minimal Electron application to test induction motor",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-installer": "electron-builder",
    "package-win": "electron-packager . induction-motor-testing-app --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Induction Motor Testing\"",
    "package-linux": "electron-packager . induction-motor-testing-app --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/linux/icon.png --prune=true --out=release-builds"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^28.2.2",
    "electron-packager": "^17.1.2"
  }
}
